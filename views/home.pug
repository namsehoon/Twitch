extends layouts/main
include mixins/videoBlock
include mixins/imageBlock

block content
    .home-videos
        each video in videos
            +videoBlock({
                id:video.id,
                videoFile:video.fileUrl,
                title:video.title,
                views:video.views,
                game:video.game
            })
        each video in videos
            +imageBlock({
            imageFile:video.imageFileUrl
            })
